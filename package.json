{
  "name": "ylvonis",
  "version": "3.0.3",
  "description": "Node express mongoose typescript boilerplate",
  "main": "dist/index.js",
  "bin": "bin/createNodetsApp.cjs",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=14.0.0 <19"
  },
  "scripts": {
    "tsc": "./node_modules/typescript/bin/tsc",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "start:build": "tsc --build && pm2 start ecosystem.config.json --no-daemon",
    "compile": "tsc --build",
    "compile:watch": "tsc --build --watch",
    "pre:dev": "cross-env NODE_ENV=development nodemon --experimental-modules --es-module-specifier-resolution=node dist/index.js",
    "dev": "concurrently --kill-others \"yarn compile:watch\" \"yarn pre:dev\""
  },
  "keywords": [
    "node",
    "typescript",
    "node.js",
    "boilerplate",
    "generator",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "2.8.8",
    "@types/express-rate-limit": "^5.1.3",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.12",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.0",
    "concurrently": "^7.1.0",
    "coveralls": "^3.1.1",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@adminjs/express": "^5.1.0",
    "@adminjs/mongoose": "^3.0.1",
    "@sindresorhus/slugify": "^2.2.0",
    "@types/uuid": "^9.0.0",
    "adminjs": "^6.8.4",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.33.0",
